package basics;

public class Student {
//	Static Code Analysis : ESLint totalMarks   
//	total_marks XXXX
//	static and final constants : all capitals
//	classs : start with capital
//	method, variable name : start with small letter
	// GO BACK ALT <-
	
	
	public String name;
	public int roll;
	public String branch;
	
	@Override
	public String toString() {
		return "Student [name=" + name + ", roll=" + roll + ", branch=" + branch + "]";
	}
	
	
}
